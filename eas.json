{
  "cli": {
    "version": ">= 3.1.1",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://tfmtracker.azurewebsites.net"
      },
      "channel": "production"
    }
  }
}
